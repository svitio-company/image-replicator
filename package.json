{
  "name": "image-replicator",
  "version": "1.0.0",
  "description": "Kubernetes Validating Webhook to verify container images exist in registries",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "benchmark": "bun run benchmark.ts",
    "type-check": "bunx tsc --noEmit",
    "generate-certs": "bash scripts/generate-certs.sh"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  }
}
